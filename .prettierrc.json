{
  "printWidth": 110,
  "singleQuote": true,
  "semi": false,
  "tabWidth": 4,
  "trailingComma": "es5",
  "jsxSingleQuote": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": "*.md",
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": "*.scss",
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": "*.css",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
